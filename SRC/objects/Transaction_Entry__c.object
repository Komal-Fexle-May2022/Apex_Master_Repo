    <validationRules>
        <fullName>Contact_Status_Inactive</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISPICKVAL( Contact__r.Contact_Status__c , &quot;Inactive&quot;))</errorConditionFormula>
        <errorMessage>Contact Is Inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Does_Not_Have_Enough_Balance</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Contact__r.Available_Balance__c &lt;= 3000, ISPICKVAL( Type__c , &quot;Debit&quot;))</errorConditionFormula>
        <errorMessage>Available Balance is not enough.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Insufficient_Balance</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Contact__r.Available_Balance__c &lt; Amount__c , ISPICKVAL( Type__c , &quot;Debit&quot;) )</errorConditionFormula>
        <errorMessage>Insufficient Balance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Maximum_Debit_Allowed_Per_Transaction</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Contact__r.Transaction_Limit__r.Maximum_Debit_Allowed_Per_Transaction__c &lt;  Amount__c , ISPICKVAL( Type__c , &quot;Debit&quot;) )</errorConditionFormula>
        <errorMessage>You cannot debit amount .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Date_must_Be_Today</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW()  &amp;&amp;  Transaction_Date__c &lt;&gt; TODAY()</errorConditionFormula>
        <errorMessage>Transaction Date must be of today.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
